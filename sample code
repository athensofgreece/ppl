            }
        }

       <| IF STMT :>
        if (#x < 10) { 
            buzzout("x is less than 10"); 
        }

        if (#y == 0) { 
            buzzout("y is zero");
        } else { 
            buzzout("y is non-zero"); 
        }

        if (#z > 10) { 
            buzzout("z is greater than 10"); 
        } else if (#z == 10) { 
            buzzout("z equals 10"); 
        } else { 
            buzzout("z is less than 10"); 
        }

     
