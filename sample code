                buzzout("i: %d, j: %d", #i, #j);
            }
        
        while (#x < 5) {
            buzzout("Current value: %d", #x);
            #x = #x + 1;
        }

        while (#x < 10) { 
            while (#y < 10) { 
                buzzout("%d ", #y); 
                #y++; 
            }
            #x++;
        }

       
